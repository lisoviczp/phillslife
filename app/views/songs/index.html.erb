<head>
<style>

	body {
		background: url('/assets/icelandbeach.jpg');
		background-size: 100%;
		background-attachment: fixed;
	}

</style>
</head>

<body>

<br>
<link href='http://fonts.googleapis.com/css?family=Megrim' rel='stylesheet' type='text/css'>

<h1> vibes </h1>
<hr>

<div class="about-wrapper">
	<table>
		<tr id="table-header">
			<th> track </th>
			<th> artist </th>
			<th> vibe </th>
		</tr>
		<% @song.reverse.each do |song| %>
			<% if song.approved %>
				<div class="item">
						<tr>
							<th> <span class="signature"> <%= song.title %> </span> </th>
							<th> <%= song.body %>  </th>
							<th> <%= link_to image_tag("glyphicons-77-headphones.png"), song_path(song) %> </th>
						</tr>
				</div>
			<% end %>
		<% end %>
	</table>
</div>

</body>
</html>
