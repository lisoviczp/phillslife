<style>

.background_image {
	border-bottom: solid;
	border-width: 3px;
	padding-bottom: 3px;
}


</style>


<h1>all-timers</h1>
<h3> <%= link_to 'current', songs_current_path %> </h3>

<div class="background_image">
    <img src="http://s3.amazonaws.com/phillslife-assets/pictures/images/000/000/002/original/Nantucket_Pan.JPG?1417479373" alt="Nantucket" style="width:100%;height:400px;">
</div>

<% @song.reverse.each do |song| %>
	<% if song == @song.reverse.first %>
		<div class="firstPost">
			<iframe width="100%" height="450" scrolling="no" frameborder="no" src=  <%= %Q"#{song.url}" %> ></iframe>
		</div>
		<br>
	<% else %>
		<div class="posts">
			<iframe width="100%" height="450" scrolling="no" frameborder="no" src=  <%= %Q"#{song.url}" %> ></iframe>
		</div>
	<% end %>	
	<br>
<% end %>
